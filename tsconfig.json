{
  "compilerOptions": {
    "allowSyntheticDefaultImports": true,
    "jsx": "react-native",
    "lib": ["dom", "esnext"],
    "moduleResolution": "node",
    "noEmit": true,
    "skipLibCheck": true,
    "target": "esnext",
    "noImplicitAny": false,
    "baseUrl": "./src",

    "paths": {
      "src/*": ["./*"]
    },
    "resolveJsonModule": true
  }
}
